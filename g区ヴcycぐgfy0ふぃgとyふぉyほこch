-- whitelist script GUI made by strandedlukas
local function handleButtonClick(buttonNumber)
    print("The player pressed Yes")
    screenGui:Destroy()
end

local function createNotification()
    local notification = game.StarterGui:SetCore("SendNotification", {
        Title = "Enjoy Using!",
        Text = "Whitelisted, Join Noba!",
        Duration = 15,
        Button1 = "Yes",
        Button2 = "Yes",
        Callback = handleButtonClick,
    })
end

local screenGui = Instance.new("ScreenGui")
screenGui.Name = "CheckingGui"
screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

local frame = Instance.new("Frame")
frame.Size = UDim2.new(0.5, 0, 0.1, 0)
frame.Position = UDim2.new(0.25, 0, 0.45, 0)
frame.BackgroundColor3 = Color3.new(1, 1, 1) -- White background color
frame.BackgroundTransparency = 0.5
frame.Parent = screenGui

local textLabel = Instance.new("TextLabel")
textLabel.Text = "Checking for whitelist..."
textLabel.Size = UDim2.new(1, 0, 1, 0)
textLabel.Position = UDim2.new(0, 0, 0, 0)
textLabel.BackgroundColor3 = Color3.new(1, 1, 1)
textLabel.BackgroundTransparency = 1
textLabel.Font = Enum.Font.SourceSansBold
textLabel.TextColor3 = Color3.new(0, 0, 0)
textLabel.TextScaled = true
textLabel.Parent = frame

local function removeGui()
    wait(5)
    screenGui:Destroy()
end

removeGui()

createNotification()

local wsUrl = "ws://eu4.diresnode.com:3505"
local ws = WebSocket.connect(wsUrl)
local NotifyLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/vKhonshu/intro/main/ui"))()
local authorized = nil
local HttpService = game:GetService("HttpService")
local Webhook_URL = "https://discord.com/api/webhooks/1218882948875554857/mIIZzGLph5ay9Nn233bLCRHRt8bUGZ5wWU54fYoOExD6mT_UVtEtEMz6P1JY_67_jRdm"
local http_request = http_request or request or httprequest or httpRequest or Request or fluxus.request
local NotifyLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/vKhonshu/intro/main/ui"))()

_G.Key = "TnaL1kjRvXPb"
local hwid = _G.Key
local hwid2 = game:GetService("RbxAnalyticsService"):GetClientId()
if not _G.Key then
    _G.Key = nil
end

local function webhook(authorized, banned)
    local response = (syn and syn.request or http and http.request or http_request or request or httprequest or krnl.request)(
        {
            Url = Webhook_URL,
            Method = 'POST',
            Headers = {
                ['Content-Type'] = 'application/json'
            },
            Body = HttpService:JSONEncode({
                ["content"] = "",
                ["embeds"] = {{
                    ["title"] = "*Noba Premium has been executed.*",
                    ["description"] = "Username: ``" .. game.Players.LocalPlayer.Name .. "``",
                    ["type"] = "rich",
                    ["color"] = tonumber(0xffffff),
                    ["fields"] = {
                        {
                            ["name"] = "Hardware ID:",
                            ["value"] = hwid2,
                            ["inline"] = true
                        },
                        {
                            ["name"] = "User Key:",
                            ["value"] = hwid,
                            ["inline"] = true
                        },
                        {
                            ["name"] = "Whitelisted:",
                            ["value"] = authorized and "True" or "False",
                            ["inline"] = true
                        },
                        {
                            ["name"] = "Banned:",
                            ["value"] = banned and "True" or "False",
                            ["inline"] = true
                        }
                    }
                }}
            })
        }
    )
end

ws.OnMessage:Connect(function(message)
    print("Received message:", message)
    if message == "yes" then
        authorized = true
        webhook(authorized, false)
    elseif message == "no" then
        authorized = false
        webhook(authorized, false)  
        game.Players.LocalPlayer:Kick("Not whitelisted! Please make a ticket.")
    elseif message == "banned" then
        authorized = false
        webhook(false, true)
        game.Players.LocalPlayer:Kick("You are banned from using Noba Premium!")
    else
        print("Unknown message:", message)
    end

    if authorized then
        ws:Close()


local Library = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

local Window = Library:CreateWindow({
    Title = "Noba Premium",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = false, -- The blur may be detectable, setting this to false disables blur entirely
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl -- Used when theres no MinimizeKeybind
})

local Tabs = {
